<html>
    <head>
        <title>Depts Page - Express App</title>
    </head>
    <body bgcolor="LightPink">
            <h2>All Departments Details</h2>
            <hr/>

        <table border="2">
            <tr>
                <th>Deptno</th>
                <th>Dname</th>
                <th>Location</th>
            </tr>    
            <%  for(var i in data) { %>
                <tr>
                       <td> <%= data[i].deptno  %> </td> 
                       <td> <%= data[i].dname  %> </td> 
                       <td> <%= data[i].loc  %> </td> 
                </tr>
            <%  } %>
        </table>    
             
                
    </body>
</html>
